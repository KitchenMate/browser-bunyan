{
  "name": "browser-bunyan",
  "version": "1.3.0",
  "description": "Bunyan logging specifically for the browser",
  "author": "Phil Mander <philip.mander@gmail.com>",
  "main": "./lib/index.js",
  "module": "./lib/index.m.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/philmander/browser-bunyan.git"
  },
  "keywords": [
    "log",
    "logging",
    "log4j",
    "json",
    "bunyan"
  ],
  "scripts": {
    "lint": "eslint src",
    "prepublish": "npm run lint && npm test && npm run build",
    "build": "microbundle --name bunyan && mkdirp dist && cp lib/index.umd.js dist/browser-bunyan.min.js",
    "test": "babel-tap test/*.test.js"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.5.2",
    "babel-tap": "^5.0.0",
    "eslint": "^4.5.0",
    "microbundle": "^0.2.4",
    "mock-xmlhttprequest": "^1.1.0"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/philmander/browser-bunyan/blob/master/LICENSE"
    }
  ],
  "babel": {
    "presets": [
      "env"
    ]
  }
}
